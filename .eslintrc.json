{
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb"
  ],
  "parser": "@typescript=eslint/parse",
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "rules": {
    "react/prefer-stateless-function": "off",
    "jsx-quotes": "off", // 单引号
    "linebreak-style": "off", // 换行符
    "semi": "off" // 分号
  }
}